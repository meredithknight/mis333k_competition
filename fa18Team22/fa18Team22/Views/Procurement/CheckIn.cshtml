
<h2>

    <span style="font-size: 3em; color: lightgrey;">
        <i class="fas fa-truck-loading"></i>
    </span>

Procurement</h2>
<div>
    <p><font color="red">@ViewBag.quantitycheck</font></p>
</div>


<div class="row">
    <div class="col-md-4">
        <form asp-action="CheckIn">
            <div>
                Incoming Quantity
            </div>

            <div class="form-group">
                <input name="IncomingQuantity" class="form-control" />
            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-success">Check In <i class="fas fa-plus-circle"></i></button>
            </div>
        </form>
    </div>
</div>
